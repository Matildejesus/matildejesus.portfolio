         <script>
            ScrollReveal().reveal(".tool", { delay: 700 });
            ScrollReveal().reveal(".card1", {
                beforeReveal: function (el) {
                el.classList.add(
                    "animate__animated",
                    "animate__jackInTheBox",
                    "animate__delay-1s"
                );
                },
            });
            ScrollReveal().reveal(".card2", {
